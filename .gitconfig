[user]
  name = maejimayuto
  email = maejimayuto@gmail.com
[core]
  excludesfile = /Users/yuto.maejima/.gitignore_global
  quotepath = false
  editor = vim
  attributesfile = /Users/yuto.maejima/.gitattributes_global
[difftool "sourcetree"]
  cmd = opendiff \"$LOCAL\" \"$REMOTE\"
  path =
[mergetool "sourcetree"]
  cmd = /Applications/Sourcetree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
  trustExitCode = true
[commit]
  template = /Users/yuto.maejima/.stCommitMsg
[filter "lfs"]
  clean = git-lfs clean -- %f
  smudge = git-lfs smudge -- %f
  process = git-lfs filter-process
  required = true
[pager]
  log = diff-highlight | less
  show = diff-highlight | less
  diff = diff-highlight | less
[interactive]
  diffFilter = diff-highlight
[alias]
  lo = log --oneline --graph --decorate --branches --date="iso" --abbrev-commit --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset'
  dif = diff --word-diff --ignore-space-change
  ck = checkout
  s = status --short --branch
  co = commit
  a = add -A
[credential]
  helper = cache
[url "github:"]
  InsteadOf = https://github.com/
  InsteadOf = git@github.com:
[color]
  ui = auto
